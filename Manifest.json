{
 "$schema": "https://qooxdoo.org/schema/Manifest-2-0-0.json",
  "info" :
  {
    "name" : "themedemo",
    "summary" : "Common used from demo",
    "description" : "This is a demo containing a different widgets to show theme features.",
    "homepage" : "",
    "license" : "MIT license",
    "authors" : [
      {
        "name" : "Norbert Schr√∂der (scro34)",
        "email" : "schroeder@scro34.de"
      },
      {
        "name": "Dmitrii Zolotov (goldim)",
        "email": "zolotovdy@yandex.ru"
      }
],
    "version" : "1.0.0"
  },

  "provides" :
  {
    "namespace"   : "qxl.themedemo",
    "encoding"    : "utf-8",
    "class"       : "source/class",
    "resource"    : "source/resource",
    "translation" : "source/translation",
    "fonts": {
      "JosefinSlab Bold": {
        "fontFaces": [
          {
            "paths": [
              "qxl/themedemo/font/JosefinSlab-Bold.ttf"
            ]
          }
        ]
      },
      "JosefinSlab Regular": {
        "fontFaces": [
          {
            "paths": [
              "qxl/themedemo/font/JosefinSlab-Regular.ttf"
            ]
          }
        ]
      }
    }
  },

  "requires": {
    "@qooxdoo/framework": "^7.6.3",
    "qooxdoo/qxl.widgetbrowser": "^2.0.0",
    "qooxdoo/qxl.formdemo": "^2.0.0",
    "qooxdoo/qxl.versionlabel": "^2.0.0"
  }
}
